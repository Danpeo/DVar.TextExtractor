<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="DVar.TextExtractor.Gui.App"
             xmlns:local="using:DVar.TextExtractor.Gui"
             RequestedThemeVariant="Dark">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="avares://DVar.TextExtractor.Gui/Icons.axaml"></StyleInclude>
        <StyleInclude Source="avares://DVar.TextExtractor.Gui/Colors.axaml"></StyleInclude>
    </Application.Styles>
</Application>